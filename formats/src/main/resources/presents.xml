<?xml version="1.0" encoding="UTF-8"?>
<pr:presents xmlns:pr="http://www.ubrr.org/presents"
             xmlns:s="http://www.ubrr.org/sock"
             xmlns:c="http://www.ubrr.org/card"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="
              http://www.ubrr.org/presents
              ./xsd/presents.xsd">

  <pr:present>
    <pr:sock s:size="4">
      <s:color>red</s:color>
    </pr:sock>
    <pr:card>
      <c:message>Happy New Year!</c:message>
      <c:color>gold</c:color>
    </pr:card>
  </pr:present>
  
</pr:presents>
